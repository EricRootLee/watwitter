<h1>Show Post</h1>

<ul>

  <li>
    <strong>Username:</strong>
    <%= @post.username %>
  </li>

  <li>
    <strong>Body:</strong>
    <%= @post.body %>
  </li>

  <li>
    <strong>Likes count:</strong>
    <%= @post.likes_count %>
  </li>

  <li>
    <strong>Reposts count:</strong>
    <%= @post.reposts_count %>
  </li>

</ul>

<span><%= live_redirect "Back", to: Routes.post_index_path(@socket, :index) %></span>
